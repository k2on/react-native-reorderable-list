{
  "name": "reorderablelistexample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "cd ../ && rm -f ./react-native-reorderable-list-0.4.0.tgz && npm pack && cd ./example",
    "android": "react-native run-android",
    "build:android": "mkdirp dist/res && react-native bundle --entry-file index.js --platform android --dev true --bundle-output dist/main.android.jsbundle --assets-dest dist/res",
    "build:ios": "mkdirp dist && react-native bundle --entry-file index.js --platform ios --dev true --bundle-output dist/main.ios.jsbundle --assets-dest dist",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.67.4",
    "react-native-gesture-handler": "2.8.0",
    "react-native-reanimated": "2.10.0",
    "react-native-reorderable-list": "file:../react-native-reorderable-list-0.4.0.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "mkdirp": "^1.0.0",
    "react-native-test-app": "^2.1.1",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
